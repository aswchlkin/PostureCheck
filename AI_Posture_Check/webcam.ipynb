    "import cv2\n",
    "import time\n",
    "\n",
    "camera = cv2.VideoCapture(0)\n",
    "while True :\n",
    "    return_value, image = camera.read()\n",
    "    cv2.imwrite('picture.jpg', image)\n",
    "    time.sleep(0.5)\n",
    "del(camera)"
